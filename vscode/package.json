{
  "name": "star-console",
  "displayName": "Star Console",
  "publisher": "MateusPitura",
  "description": "This extension is built to ease inserting log statements integrated with Chrome extension",
  "repository": "https://github.com/MateusPitura/extension-typescript-console",
  "version": "0.1.4",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.95.0"
  },
  "activationEvents": [
    "onCommand:star-console.insertLogStatement"
  ],
  "categories": [
    "Debuggers"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "star-console.insertLogStatement",
        "title": "Add log statement"
      }
    ],
    "keybindings": [
      {
        "command": "star-console.insertLogStatement",
        "key": "shift+alt+l",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^22.5.0",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^10.7.3",
    "typescript": "^5.5.4",
    "vscode-test": "^1.6.1"
  }
}
